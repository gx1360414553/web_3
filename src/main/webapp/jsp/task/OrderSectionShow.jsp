<%@page import="com.alibaba.fastjson.JSONObject"%>
<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt" %>
<%@ taglib uri="/tags-cx" prefix="cx" %>
<div init-model='orderSection'>
<%=JSONObject.toJSONString( request.getAttribute( "orderSection" ) )%>
</div>
<form class="form-horizontal ng-pristine ng-valid" role="form">
<div class="row" style="max-height: 500px;overflow: auto;">

	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">主键</label>
	   <div class="col-sm-10">
	   		{{orderSection.orderSectionId}}
	   </div>
	</div>
	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">下单开始时间</label>
	   <div class="col-sm-10">
	   		{{orderSection.startTime | date:'yyyy-MM-dd HH:mm:ss'}}
	   </div>
	</div>
	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">下单结束时间</label>
	   <div class="col-sm-10">
	   		{{orderSection.endTime | date:'yyyy-MM-dd HH:mm:ss'}}
	   </div>
	</div>
	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">预计送达时间</label>
	   <div class="col-sm-10">
	   		{{orderSection.deliveryTime | date:'yyyy-MM-dd HH:mm:ss'}}
	   </div>
	</div>
	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">任务标记</label>
	   <div class="col-sm-10">
	   		{{orderSection.mark}}
	   </div>
	</div>
	<div class="form-group col-lg-6 col-xs-6 col-sm-12">
	   <label for="inputEmail3" class="col-sm-2 no-padding-right" style="text-align: right;">起始编号</label>
	   <div class="col-sm-10">
	   		{{orderSection.startNo}}
	   </div>
	</div>

</div>
</form>